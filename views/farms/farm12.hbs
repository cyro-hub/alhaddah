<body class="p-3 mb-2">
    <div class="modal" tabindex="-1" id="delete">
        <div class="modal-dialog">
            <div class="modal-content">
                <i class="fa fa-question text-danger" id="icon"></i>
                <div class="modal-body">
                    <p id="deleteMessage"></p>
                </div>
                <div class="footer">
                    <button id="yes" class="btn btn-primary">Yes</button>
                    <button id="no" class="btn btn-danger">No</button>
                </div>
            </div>
        </div>
    </div>
    <a data-toggle="modal" data-target="#delete" id="openDelete"></a>
    {{!-- end of verification --}}

    {{!-- response  --}}
    <div class="modal" tabindex="-1" id="balanceSuccess">
        <div class="modal-dialog">
            <div class="modal-content">
                <i class="fa fa-check text-success" id="success"></i>
                <div class="modal-body">
                    <p id="successMessage"></p>
                </div>
                <div class="footer">
                    <button id="okSuccess" class="btn btn-primary" data-dismiss="modal" aria-label="Close">ok</button>
                </div>
            </div>
        </div>
    </div>
    <a data-toggle="modal" data-target="#balanceSuccess" id="openSuccess"></a>
    <div class="modal" tabindex="-1" id="balanceWarning">
        <div class="modal-dialog">
            <div class="modal-content">
                <i class="fa fa-exclamation text-danger" id="warning"></i>
                <div class="modal-body">
                    <p id="warningMessage"></p>
                </div>
                <div class="footer">
                    <button id="okWarning" class="btn btn-primary" data-dismiss="modal" aria-label="Close">ok</button>
                </div>
            </div>
        </div>
    </div>
    <a data-toggle="modal" data-target="#balanceWarning" id="openWarning"></a>
    {{!-- end of response --}}
    <table class="table table-hover table-sm">
